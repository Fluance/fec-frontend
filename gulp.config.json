{
    "client": "./src/client/",
    "server": "./src/server/",
    "allscss": [
        "./src/client/app/**/*.scss",
        "./src/client/content/scss/**/*.scss"
    ],
    "scss": "./src/client/content/scss/styles.scss",
    "sourcemapsdest": "../maps",
    "cssroot": "./src/client/content/css/",
    "css": [
        "./src/client/content/css/styles.css"
    ],
    "configfile": "./src/client/config.js",
    "js": [
        "src/client/app/**/*.module.js",
        "src/client/app/**/*.js",
        "!src/client/app/**/*.spec.js"
    ],
    "alljs": [
        "./src/client/app/**/*.js",
        "./src/server/**/*.js",
        "./test/test-helpers/**/*.js"
    ],
    "viewers": {
        "base": "./src/client/viewers/",
        "pdfViewerFiles_bower": [
            "bower_components/pdf.js-viewer/viewer.css",
            "bower_components/pdf.js-viewer/viewer-overwrites.css",
            "bower_components/pdf.js-viewer/images/**/*",
            "bower_components/pdf.js-viewer/pdf.js",
            "bower_components/pdf.js-viewer/pdf.worker.js",
            "bower_components/angular/angular.js",
            "bower_components/angular-pdfjs-viewer/dist/angular-pdfjs-viewer.js"
        ],
        "pdfViewerFiles": [
            "node_modules/pdf.js-dist/viewer.css",
            "node_modules/pdf.js-dist/viewer-overwrites.css",
            "node_modules/pdf.js-dist/images/**/*",
            "node_modules/pdf.js-dist/pdf.js",
            "node_modules/pdf.js-dist/pdf.worker.js",
            "node_modules/angular/angular.js",
            "node_modules/angular-pdfjs-viewer/dist/angular-pdfjs-viewer.js"
        ],
        "pdfViewerInjections": {
            "css": [
                "pdf.js-dist/viewer.css",
                "pdf.js-dist/viewer-overwrites.css"
            ],
            "js": [
                "angular/angular.js",
                "pdf.js-dist/pdf.js",
                "pdf.js-dist/pdf.worker.js",
                "angular-pdfjs-viewer/dist/angular-pdfjs-viewer.js"
            ]
        }
    },
    "fonts": [
        "./src/client/content/fonts/**/*.*",
        "./node_modules/components-font-awesome/fonts/**/*.*"
    ],
    "images": [
        "./src/client/content/images/**/*.*"
    ],
    "static_pages": [
        "./src/client/content/**/*.html"
    ],
    "build": "./dist/",
    "test_results": "./test_results/",
    "temp": "./.tmp/",
    "report": "./report/",
    "filestowatch": [
        "./src/client/**/*.js",
        "./src/client/**/*.css",
        "./src/client/**/*.html",
        "./src/client/**/*.json",
        "./src/client/content/images/**/*.*"
    ],
    "lang": "./src/client/app/**/lang/*.json",
    "fallbackLang": "./src/client/app/*/lang/*.en.json",
    "unbundled": [
        "./src/client/app/**/lang/*.json",
        "./src/client/app/**/dashboard/components/**/*.service.js",
        "./src/client/app/**/dashboard/components/**/*.tpl.*.html",
        "./src/client/app/**/dashboard/components/**/*.tpl.html",
        "./src/client/app/**/layout/shell*.html"
    ],
    "targets_DEPRECATED": {
        "fluance": {
            "name": "fluance",
            "index": "index.html",
            "templateRoot": "app/",
            "htmlTemplates": "./src/client/app/**/*.html",
            "exclusions": [
                "bower_components/bootstrap/",
                "bower_components/angular-bootstrap/",
                "bower_components/google-code-prettify",
                "bower_components/Sortable/knockout-sortable.js",
                "bower_components/Sortable/react-sortable-mixin.js",
                "bower_components/Materialize/bin/materialize.css"
            ]
        }
    }
}
